# -*- coding: utf-8 -*-
"""
wei lai
客户资源池中的常量定义
"""


class PoolProperty (object):
    """
    wei lai
    资源池状态及标签定义
    """
    shared = u"shared"   # 共享
    unshared = u"unshared"   # 独享
    assigned = u"assigned"  # 已分配
    unassigned = u"unassigned"    # 未分配
    enable = u"enable"   # 可用
    disable = u"disable"  # 不可用
    vmware = u"VMware"
    openstack = u"Openstack"


class ClusterStatus (object):
    """
    底层资源状态
    """
    enabled = u"enabled"  # 可用
    disabled = u"disabled"  # 不可用


